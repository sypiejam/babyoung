		<script>
			//팝업 툴팁 뉴타입 추가
			$(document).on('click', '.btn-tooltip', function(){
				var idx = $(this).parent();
				toolTipClose();
				idx.addClass("on");
				var idxW = idx.find(".tooltip-box").offset().left - 20;
				idx.find(".tooltip-box").css("left","-"+idxW+"px")
			});
			function toolTipClose(){
				$(".tooltip-area").removeClass("on");
				$(".tooltip-box").css("left",0)
			}
            // 24-09-11 IRP2024 : 상품변경 바텀시트 ID값 추가
			// 24-09-26 IRP2024 : ID값 클래스로 변경
            $(document).on("click", ".irp24-bottomsheet-tooltip", function(){
				var target = $(".btn-tooltip");
				var clickDiv = event.target;
				if(!target.is(clickDiv)){
					toolTipClose();
				}
			});

			const myModalObj01 = new UI.ModalPopup("#modalPopup01"); //평가금액 툴팁

			const topBtn = document.querySelector(".ui-btn-top");
			window.addEventListener("scroll", () => {
				if (window.pageYOffset > 100) {
					topBtn.classList.add("view");
				} else {
					topBtn.classList.remove("view");
				}
			});
			function moveTop() {
				window.scrollTo({ top: 0, behavior: "smooth" });
			}

			// 24-10-17 IRP2024 : 선정하는 이유 펼치기/접기 버튼 추가
			$(document).on('click', '.irp24-operation-mall .box-more-view .more', function(){
				var el = $(this).parents(".box-more-view").find(".text-wrap");
				if ($(this).hasClass("on")) {
					$(this).removeClass("on").find("span").text("펼치기");
					el.removeClass("on");
				} else {
					$(this).addClass("on").find("span").text("접기");
					el.addClass("on");
				}
			});
			// 24-11-08 IRP2024 : 2줄 이하 일때 버튼 숨기기 추가
			$('.banner-fund').click(function(){
				setTimeout(function(){
					$(".box-more-view .text-wrap").each(function(){
						$(this).addClass("on")
						var idHeight = $(this).height();
						if(idHeight > 70){
							$(this).removeClass("on");
						}else{
							$(this).next(".button-center").hide();
						}
					});
				},50)
			});
		</script>
