import "slick-carousel/slick/slick.css";
import "slick-carousel/slick/slick-theme.css";
import styled from "styled-components";

/**
 * 
 * @name SlickFrame 
 * @role slick 최상위 컴포넌트입니다.
 * 
 */

function SlickFrame({ children }) {

    return (
        <StyleSlickWrap>
            {children}
        </StyleSlickWrap>
    );
}


const StyleSlickWrap = styled.div`
    position:relative;
    width:100%;
    margin:0 auto;
`;

export default SlickFrame;
