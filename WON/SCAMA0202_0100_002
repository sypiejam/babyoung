<script>
	function landingHSet(){
		var winH = $(window).height();
		var headerH = $('.nst_header').innerHeight();
		var fixedH = $('.nst_buttons_wrap').innerHeight();
		var textH = $('.owl-item .nst_top_con').outerHeight(true);
		var dotsH = $('.owl-dots').outerHeight(true);
		var imgH = winH - headerH - fixedH - textH - dotsH;
		$('.nst_tutorial_imgbox').css({'height' : imgH});
		$('.nst_buttons_wrap').addClass('fixed').css('opacity','1');
	}

	function landingSlider(){
		var owlLanding = $('.nst_tutorial_wrap .owl-carousel');
		var owlLandingItem = $('.nst_tutorial_wrap .owl-carousel .item').length;
		var owlLandingOpt = {
			nav: false,
			dots: true,
			items: 1,
			loop : false,
			onTranslated : function(evt){
				var idx = evt.relatedTarget.relative(evt.item.index) + 1;
				if(idx === 4){
					$('.nst_tutorial_wrap lottie-player').get(0).play();
				} else {
					$('.nst_tutorial_wrap lottie-player').get(0).stop();
				}
			}
		}
		if(owlLandingItem > 1) {
			owlLanding.owlCarousel(owlLandingOpt);
		} 
	}

	$(document).ready(function(){
		landingSlider();
		landingHSet();
	})

	$(window).on({
		'resize' : function(){
			landingHSet();
		},
		'load' : function(){
			$('.nst_tutorial_wrap').addClass('loaded');
		}
	})
</script>
