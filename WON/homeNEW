						<div class="-main_skeleton_panel">
							<div class="nst_svclink_icon" aria-hidden="true"><img src="/img/nonetouch/pib/_new2/icon/ic_opentypehome_payment.png" alt=""></div>
							<span class="nst_svclink_text">선불 서비스</span>
						</div>
					</a>
				</li>
				<li class="nst_svclink_list">
					<a href="javascript:;" class="nst_svclink_item">
						<div class="-main_skeleton" aria-label="로딩중"></div>
						<div class="-main_skeleton_panel">
							<div class="nst_svclink_icon" aria-hidden="true"><img src="/img/nonetouch/pib/_new2/icon/ic_opentypehome_indice.png" alt=""></div>
							<span class="nst_svclink_text">주식매매</span>
						</div>
					</a>
				</li>
			</ul>
		</div>
	</div>
	<!-- //생활혜택(WSM)[E] -->

</div>
<!-- //공통 레이아웃 (frame_container) -->

<script>
	$(document).ready(function(){
		wrappingMain();
	});
	var wrappingMain = function(){
		$('.frame_container').wrapAll('<div class="nst_main" />');
	};

	//간편회원 개편[S]
	$(function(){
		let winWid = window.innerWidth;
		let sldWid = $('.nst_shortcut_slide').width() - 20;

		//슬라이드 정렬 기준
		if(winWid < sldWid){
			$('.nst_shortcut_slide').css({'right' : 0});
		} else {
			$('.nst_shortcut_slide').addClass('nst_static');
		}

		//스켈레톤
		$('.-main_skeleton_panel').hide();
		setTimeout(function(){
			$('.nst_main .-main_skeleton').fadeOut();
			$('.-main_skeleton_panel').fadeIn();

			//스켈레톤 사라진 후 슬라이드 적용
			if(winWid < sldWid){
				shortcutSlide.active()
			}
		},500);

		//주요서비스 슬라이드
		let shortcutSlide = {
			active : function(){
				$('.nst_shortcut_slide').removeClass('nst_static');
				$('.nst_shortcut').addClass('nst_motion');
				$('.nst_motion .nst_shortcut_slide').css({
					'right' : 0,
					'-webkit-transform' : 'translateX(calc(100% - ' + winWid + 'px))',
					'transform' : 'translateX(calc(100% - ' + winWid + 'px))'
				});
				setTimeout(function(){
					$('.nst_motion .nst_shortcut_slide').addClass('nst_static');
				},1600);
			},
			remove : function(){
				$('.nst_shortcut').removeClass('nst_motion');
				$('.nst_shortcut_slide').removeAttr('style');
			}
		}
		$(window).on('resize', function(){
			winWid = window.innerWidth;
			if(winWid < sldWid){
				shortcutSlide.active();
			}else{
				shortcutSlide.remove();
			}
		});

		//환율 롤링
		let rollingData = {
			curIdx : 1,
			init : function(){
				$('.nst_rollbox_item:first-child').css({top:0})
			},
			rollingBody : function(){
				let preIdx = rollingData.curIdx, maxLen = 2;
				let contItem = $('.nst_maindex_body .nst_rollbox_item');
				(preIdx < maxLen) ? rollingData.curIdx++ : rollingData.curIdx = 1;
				contItem.filter(':nth-child('+ preIdx + ')').attr({'aria-hidden':true}).css({top: 0}).stop().animate({top:'-100%', opacity:0}, 400, 'easeOutQuad', function(){
					setTimeout(function(){
						contItem.filter(':nth-child('+ preIdx + ')').css({top: '100%'})
					},500)
				});
				contItem.filter(':nth-child('+ rollingData.curIdx + ')').attr({'aria-hidden':false}).delay(300).animate({top:0, opacity:1}, 400, 'easeOutQuad');
			}
		}
		setInterval(function(){
			rollingData.rollingBody();
		},5000);
		rollingData.init();
	});
	//간편회원 개편[E]


	//새소식 알람 스크립트 제어 : 뱅킹 메인과 동일
	let belllink = bodymovin.loadAnimation({
		container: document.getElementById('alrim'),
		renderer: 'svg',
		loop: false,
		autoplay: false,
		path: '/img/nonetouch/pib/_new/content/motion/ic_header_alarm.json'
	});

	belllink.addEventListener('DOMLoaded', function(){
		if ($('#alrim').hasClass('off')) {
			belllink.goToAndStop(1, true);
		} 
		else if ($('#alrim').hasClass('new')) {
			belllink.playSegments([1, 70], true);
		} 
		else if ($('#alrim').hasClass('on')) {
			belllink.goToAndStop(70, true);
		}
	});

	function doAlrimClick() {
		$('#alrim').removeClass('new');
		$('#alrim').addClass('off');
	}

	//이벤트 배너 : 뱅킹 메인과 동일
	function accActiveFocus(evt){
		$(evt.target).find('.owl-item:not('+ '.active' +')').attr('aria-hidden','true').find('*').attr('aria-hidden','true');
		$(evt.target).find('.owl-item.active').removeAttr('aria-hidden').find('*').removeAttr('aria-hidden');
	}
	function accActiveBtnFocus(obj){
		$(obj).find('.owl-item:not('+ '.active' +')').attr('aria-hidden','true').find('*').attr('aria-hidden','true');
		$(obj).find('.owl-item.active').removeAttr('aria-hidden').find('*').removeAttr('aria-hidden');
	}
	var adSlide = (function(){
		var owl_ad = $('.-js_slider_ad .owl-carousel'),
			owl_adLen = $('.-js_slider_ad .item').length;
		var financeOption = {
			loop: true,
			nav: false,
			autoplay: true,
			autoplayTimeout: 2500,
			autoplaySpeed: 400,
			dots: false,
			responsive:{0:{items: 1}},
			stagePadding: 20,
			margin:20,
			onTranslated: function(e){
				accActiveFocus(e);
				ttlCount(e);
				if( $(e.target).parents('.-js_slider_ad').find('.-control.-play').length ){
					owl_ad.trigger('stop.owl.autoplay');
				}else{
					owl_ad.trigger('stop.owl.autoplay');
					owl_ad.trigger('play.owl.autoplay');
				}
				var activeIdx = $('#adCount .-present').text() - 1;

				$(e.target).find('.owl-item').removeClass('firstBanner initActive');
			},
			onInitialized: function(e){
				accActiveFocus(e);
				ttlCount(e);
				
				//처음 로딩시 모션동작
				$(e.target).find('.owl-item.active').addClass('firstBanner');
				setTimeout(function(){
					$(e.target).find('.firstBanner').addClass('initActive');
				},100);
			}
		}
		function attachEvent(){

			//칼라지정값 처리 (객체 배열로 들어온다고함)
			var colorCode = [
				{"background":"#746fe2"},
				{"background":"#071d65"},
				{"background":"#1d870c"}
			];
			$('.-js_slider_ad').find('.item').each(function(i){
				$(this).css( colorCode[i] );
			});
			
			if( owl_adLen < 2 ){
				owl_ad.addClass('-just1');
				owl_ad.trigger('destroy.owl.carousel').removeClass('owl-carousel');
				$('.-js_slider_ad .-main_ad_remote').hide();
			}else{
				owl_ad.owlCarousel(financeOption);
				$('.-js_slider_ad .-control').addClass('-stop').attr('aria-label','일시정지');
				$('.-js_slider_ad .-slider_prev').click(function(){
					owl_ad.trigger('stop.owl.autoplay');
					owl_ad.trigger('play.owl.autoplay');
	
					owl_ad.trigger('prev.owl.carousel');
					accActiveBtnFocus(owl_ad);
				});
				$('.-js_slider_ad .-slider_next').click(function(){
					owl_ad.trigger('stop.owl.autoplay');
					owl_ad.trigger('play.owl.autoplay');
	
					owl_ad.trigger('next.owl.carousel');
					accActiveBtnFocus(owl_ad);
				});
				$('.-js_slider_ad .-control').on('click', function(){
					if( $(this).hasClass('-stop') ){
						$(this).removeClass('-stop').addClass('-play').attr('aria-label','재생');
						owl_ad.trigger('stop.owl.autoplay');
					}
					else if( $(this).hasClass('-play') ){
						$(this).removeClass('-play').addClass('-stop').attr('aria-label','일시정지');
						owl_ad.trigger('play.owl.autoplay');
					}
				});
			}
		}
		function ttlCount(evt){
			$('#adCount span').html( '<span class="-present">'+ (evt.relatedTarget.relative(evt.item.index) + 1) +'</span> / <span class="-total"> ' + evt.item.count + '</span>' );
		}
		return {
			init: function(){
				/* 스켈레톤 처리 예시(참고만) */
				setTimeout(function(){
					$('.-js_slider_ad .-main_skeleton').fadeOut();
					attachEvent();
				},500);
			}
		};
	})();
	$(function(){
		adSlide.init();
	});
</script>
</body>
</html>
